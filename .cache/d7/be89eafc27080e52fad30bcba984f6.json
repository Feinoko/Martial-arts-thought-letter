riables';\r\n// typography\r\n@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@900&family=Roboto&display=swap');\r\n\r\nbody {\r\n  font-family: 'Roboto', sans-serif;\r\n  font-size: 16px;\r\n  color: $grey;\r\n}\r\n\r\nh1 {\r\n  font-size: 16px;\r\n  font-weight: 400;\r\n  color: black;\r\n}\r\n\r\na {\r\n  color: $light-grey;\r\n}\r\n\r\n#philo-quote {\r\n  font-family: 'Montserrat', sans-serif;\r\n  line-height: 155%;\r\n  font-size: 15px;\r\n  font-weight: 900;\r\n  color: $medium-grey;\r\n  text-transform: uppercase;\r\n}\r\n\r\n#quote-author {\r\n  font-size: 8px;\r\n  color: $light-grey;\r\n  text-align: right;\r\n  font-style: italic;\r\n}\r\n\r\n#footer {\r\n  font-size: 12px;\r\n  letter-spacing: 3px;\r\n}\r\n","css\\_resets.scss":"// global resets\r\n*, *::before, *::after {\r\n  box-sizing: border-box;\r\n  margin: 0;\r\n  padding: 0;\r\n}","css\\_variables.scss":"$grey: #424242;\r\n$light-grey: #A7A7A7;\r\n$medium-grey: #555555;\r\n$marginV: 10px;","css\\styles.scss":"@import './resets';\r\n@import './typography';\r\n@import './variables';\r\n\r\n.container {\r\n  max-width: 450px;\r\n  margin: 0 auto;\r\n  border-right: 1px solid black;\r\n  border-left: 1px solid $grey;\r\n\r\n  video {\r\n    width: 100%;\r\n    height: 236px;\r\n    object-fit: cover;\r\n  } \r\n  \r\n  article {\r\n    margin: $marginV 5%;\r\n    text-align: center;\r\n    // margin: 0 5%;\r\n  }\r\n\r\n  .link-wrap {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    \r\n    a {\r\n      margin: 35px 0 $marginV 0;\r\n      text-decoration: none;\r\n      display: block;\r\n      cursor: pointer;\r\n    }\r\n  }\r\n\r\n  #twin-pics {\r\n    display: flex;\r\n    margin-bottom: $marginV;\r\n  }\r\n\r\n  .pic1 {\r\n    background-image: url(../media/bamboo.jpg);\r\n    width: 225px;\r\n    height: 282px;\r\n    background-size: cover;\r\n    background-repeat: no-repeat;\r\n  }\r\n\r\n  .pic2 {\r\n    background-image: url(../media/karateko.jpg);\r\n    width: 225px;\r\n    height: 282px;\r\n    background-size: cover;\r\n    background-position: center;\r\n    background-repeat: no-repeat;\r\n  }\r\n\r\n  #philo-quote {\r\n    margin: calc(#{$marginV}*3) 15% calc(#{$marginV}*3) 15%;\r\n  }\r\n\r\n  .quote-design-flex {\r\n    display: flex;\r\n    align-items: center;\r\n  }\r\n\r\n  #opening-quote {\r\n    height: 27px;\r\n    width: 23px;\r\n    background: url(../media/opening-quotes.JPG) no-repeat;\r\n    background-size: contain;\r\n    background-position: center;\r\n    margin-right: 10px;\r\n    // border: 1px red solid;\r\n  }\r\n\r\n  .quote-line {\r\n    background-color: $light-grey;\r\n    height: 1px;\r\n    width: 100%;\r\n    // border: 1px red solid;\r\n    display: block;\r\n\r\n  }\r\n\r\n  \r\n  #closing-quote {\r\n    height: 27px;\r\n    width: 23px;\r\n    background: url(../media/closing-quotes.JPG) no-repeat;\r\n    background-size: contain;\r\n    background-position: center;\r\n    margin-left: 10px;\r\n  }\r\n\r\n  #pattern1 {\r\n    height: 84px;\r\n    width: 100%;\r\n    background: url(../media/texture-dark.png), linear-gradient(337deg, rgba(0,0,0,1) 0%, rgb(80, 80, 80) 100%);\r\n    background-blend-mode: screen;\r\n\r\n    }\r\n\r\n  \r\n\r\n  #village {\r\n    height: 299px;\r\n    width: 450px;\r\n    background-image: url(../media/village.jpg);\r\n    background-size: contain;\r\n    margin-bottom: calc(#{$marginV}*2);\r\n  }\r\n\r\n  #footer {\r\n    \r\n    padding: 31px 0;\r\n    width: 100%;\r\n    position: relative;\r\n    text-align: center;\r\n\r\n    &::before {\r\n      content:'';\r\n      position: absolute;\r\n      top:0;\r\n      bottom:0;\r\n      left: 0;\r\n      right: 0;\r\n      opacity: 0.3;\r\n      background: url(../media/texture-dark.png);\r\n    }\r\n\r\n    span {\r\n      padding: 3px;\r\n    }\r\n\r\n    span:nth-child(odd) {\r\n      border: 1px $light-grey solid;\r\n    }\r\n\r\n  }\r\n\r\n}\r\n\r\n"},"lineCount":null}},"error":null,"hash